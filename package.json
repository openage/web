{
  "name": "pages-app",
  "version": "2.4.0",
  "scripts": {
    "ng": "ng",
    "local": "ng serve -c=local --open --port 4210",
    "dev": "ng serve -c=dev --open --port 4210",
    "qa": "ng serve -c=qa --open --port 4210",
    "prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve -c=production --open --port 4210",
    "build:local": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=local",
    "build:dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=dev",
    "build:qa": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=qa",
    "build:prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=production",
    "test": "ng test",
    "lint": "ng lint --fix oa",
    "lint:log": "ng lint --format stylish > lint.log",
    "e2e": "ng e2e",
    "analysis": "ng build && source-map-explorer dist/build/main.bundle.js",
    "serve:ssr:oa": "node dist/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.0",
    "@angular/youtube-player": "^20.0.0",
    "express": "^4.18.2",
    "font-awesome": "^4.7.0",
    "marked": "^15.0.12",
    "moment": "^2.30.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.0.0-alpha.1",
    "@angular-eslint/eslint-plugin": "^20.0.0-alpha.1",
    "@angular-eslint/eslint-plugin-template": "^20.0.0-alpha.1",
    "@angular-eslint/schematics": "^20.0.0-alpha.1",
    "@angular-eslint/template-parser": "^20.0.0-alpha.1",
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.8",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.4",
    "typescript": "~5.8.3"
  }
}
