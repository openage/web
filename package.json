{
  "name": "pages-app",
  "version": "2.4.0",
  "scripts": {
    "ng": "ng",
    "local": "ng serve -c=local --open --port 4210",
    "dev": "ng serve -c=dev --open --port 4210",
    "qa": "ng serve -c=qa --open --port 4210",
    "prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve -c=production --open --port 4210",
    "build:local": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=local",
    "build:dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=dev",
    "build:qa": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=qa",
    "build:prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=production",
    "test": "ng test",
    "lint": "ng lint --fix oa",
    "lint:log": "ng lint --format stylish > lint.log",
    "e2e": "ng e2e",
    "analysis": "ng build && source-map-explorer dist/build/main.bundle.js",
    "serve:ssr:oa": "node dist/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.7",
    "@angular/cdk": "^17.2.1",
    "@angular/common": "^17.0.7",
    "@angular/compiler": "^17.0.7",
    "@angular/core": "^17.0.7",
    "@angular/forms": "^17.2.3",
    "@angular/platform-browser": "^17.0.7",
    "@angular/platform-browser-dynamic": "^17.2.3",
    "@angular/platform-server": "^17.2.3",
    "@angular/router": "^17.2.3",
    "@angular/ssr": "^17.2.1",
    "@angular/youtube-player": "^17.2.1",
    "express": "^4.18.2",
    "font-awesome": "^4.7.0",
    "moment": "^2.30.1",
    "ngx-avatars": "^1.7.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.2.3",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^18.19.19",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.2.2"
  }
}
